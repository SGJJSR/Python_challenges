var_3 = input("Give me three variables: ")
var_3 = var_3.strip().split(" ")
print(var_3)
#var_3 = [int(i) for i in var_3]
#better:
var_3 = list(map(int, var_3)) 
print(var_3)
#var_3 =[i-1 for i in var_3]
print(var_3)
i = 1
while i <= len(var_3):
    if var_3[i+1] > var_3[i]:
        var_3.pop(i)
    i += 1
print(var_3)
